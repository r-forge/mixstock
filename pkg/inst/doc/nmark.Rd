\name{nmark}
\alias{nmark}
\alias{nmark<-}
\alias{nmix}
\alias{nmix<-}
\alias{nsource}
\alias{nsource<-}
\title{Set and query mixed stock parameters}
\description{
  Sets or queries the number of markers,
  mixed stocks, or sources, for a given mixed-stock
  data set or estimate
}
\usage{
nmark(x)
nmark(x) <- value
nmix(x)
nmix(x) <- value
nsource(x)
nsource(x) <- value
}
\arguments{
  \item{x}{a mixed-stock data object or fit}
}
\value{
  Returns the numeric value or sets the value
}
\author{Ben Bolker}
\keyword{misc}
